	"tokenColors": [
		{
			"scope": [
				"keyword",
				"keyword.control",
				"storage.modifier",
				"keyword.other.using",
				"constant.language"
			],
			"settings": {
				"foreground": "#EEB9C1"
			}
		},
		{
			"scope": [
				"variable",
				"entity.name.variable",
				"support.type.property-name",
				"punctuation.support.type.property-name"
			],
			"settings": {
				"foreground": "#DEEEF3"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#66B9D2"
			}
		},
		{
			"scope": [
				"entity.name.type",
				"entity.name.variable.enum-member",
				"keyword.type"
			],
			"settings": {
				"foreground": "#B3DCE2"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#66B9D2"
			}
		},
		{
			"scope": "entity.name.type.namespace",
			"settings": {
				"foreground": "#DEEEF3"
			}
		},
		{
			"scope": [
				"punctuation",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#B9C3C6"
			}
		},
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#666F72"
			}
		}
	]
